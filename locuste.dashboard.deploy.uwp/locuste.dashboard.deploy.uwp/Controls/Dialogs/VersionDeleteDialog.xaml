<ContentDialog
    x:Class="locuste.dashboard.deploy.uwp.Controls.Dialogs.VersionDeleteDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:locuste.dashboard.deploy.uwp.Controls.Dialogs"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="{x:Bind Title}"
    PrimaryButtonText="Fermer"
    SecondaryButtonText="Confirmer la suppression"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="VersionDeleteDialog_OnSecondaryButtonClick">

    <Grid>
        <ListView x:Name="VersionList" SelectionMode="Multiple" SelectedItem="{x:Bind Versions}"></ListView>
    </Grid>
</ContentDialog>
